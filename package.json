{
  "name": "ig-down",
  "version": "0.1.0",
  "description": "IG cli downloader.",
  "main": "index.js",
  "scripts": {
    "start": "node src/cli.js",
    "test": "eslint src build spec",
    "build": "npm run build:lib && npm run build:bundle && npm run build:executable",
    "build:bundle": "node build/bundle.js",
    "build:executable": "chmod +x bin/cli bin/cli.js",
    "build:lib": "node build/lib.js",
    "dev": "npm run build:bundle --watch"
  },
  "bin": {
    "ig-down": "bin/cli"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FranciscoKnebel/ig-down.git"
  },
  "author": "Francisco Knebel <franciscopaivaknebel@gmail.com> (https://github.com/FranciscoKnebel/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FranciscoKnebel/ig-down/issues"
  },
  "homepage": "https://github.com/FranciscoKnebel/ig-down#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.3.1",
    "commander": "^2.17.1",
    "dotenv": "^6.0.0",
    "fs-extra": "^7.0.0",
    "moment": "^2.22.2",
    "puppeteer": "^1.6.2",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "requestretry": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babelify": "^8.0.0",
    "bufferutil": "^4.0.0",
    "del-cli": "^1.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "jasmine": "^3.2.0",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "uglify-js": "^3.4.7",
    "utf-8-validate": "^5.0.1"
  }
